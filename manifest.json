{
  "manifest_version": 3,
  "name": "Browser Fingerprint Blocker",
  "version": "1.0",
  "description": "Protects against browser fingerprinting",
  "permissions": [
    "storage",
    "webRequest",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png"
  }
}